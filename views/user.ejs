<%- include('partials/header', { title: 'Member List' }) %>
<table border="1" class="table table-hover">
  <thead class="table-dark">
      <tr>
        <th>Name</th>
        <th>Gender</th>
        <th>Age</th>
        <th>Email</th>
        <th>Phone Num</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody class="table-primary">
      <% data.forEach(member => { %>
        <tr>
          <td><%= member.name %></td>
          <td><%= member.gender %></td>
          <td><%= member.age %></td>
          <td><%= member.email %></td>
          <td><%= member.tel %></td>
          <td>
            <a href="/users/delete/<%=member.id%>">Kick</a>
          </td>
        </tr>
      <% }) %>
    </tbody>
  </table>
<%- include('partials/footer') %>